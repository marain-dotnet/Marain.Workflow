{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountConnectionString": {
      "type": "string"
    },
    "runFromPackage": {
      "type": "string",
      "defaultValue": ""
    },
    "functionsAppName": {
      "type": "string",
      "minLength": 1
    },
    "applicationInsightsInstrumentationKey": {
      "type": "string",
      "minLength": 1
    },
    "leasingStorageAccountConnectionString": {
      "type": "string",
      "minLength": 1
    },
    "operationsControlServiceBaseUrl": {
      "type": "string",
      "minLength": 1
    },
    "tenancyStorageAccountName": {
      "type": "string"
    },
    "tenancyKeyVaultName": {
      "type": "string"
    },
    "tenancyStorageAccountKeyName": {
      "type": "string"
    },
    "workflowCosmosAccountUri": {
      "type": "string"
    },
    "workflowCosmosKeyVaultName": {
      "type": "string"
    },
    "workflowCosmosAccountKeySecretName": {
      "type": "string"
    },
    "workflowCosmosDatabaseName": {
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "name": "[concat(parameters('functionsAppName'),'/appsettings')]",
      "type": "Microsoft.Web/sites/config",
      "apiVersion": "2015-08-01",
      "dependsOn": [
      ],
      "tags": {
        "displayName": "AppSettings"
      },
      "properties": {
        "WEBSITE_CONTENTAZUREFILECONNECTIONSTRING": "[parameters('storageAccountConnectionString')]",
        "WEBSITE_CONTENTSHARE": "[toLower(parameters('functionsAppName'))]",
        "WEBSITE_RUN_FROM_PACKAGE": "[parameters('runFromPackage')]",
        "AzureWebJobsDashboard": "[parameters('storageAccountConnectionString')]",
        "AzureWebJobsStorage": "[parameters('storageAccountConnectionString')]",
        "APPINSIGHTS_INSTRUMENTATIONKEY": "[parameters('applicationInsightsInstrumentationKey')]",
        "FUNCTIONS_EXTENSION_VERSION": "~3",
        "FUNCTIONS_WORKER_RUNTIME": "dotnet",

        "LeasingStorageAccountConnectionString": "[parameters('leasingStorageAccountConnectionString')]",

        "TenantCloudBlobContainerFactoryOptions:RootTenantBlobStorageConfiguration:AccountName": "[parameters('tenancyStorageAccountName')]",
        "TenantCloudBlobContainerFactoryOptions:RootTenantBlobStorageConfiguration:KeyVaultName": "[parameters('tenancyKeyVaultName')]",
        "TenantCloudBlobContainerFactoryOptions:RootTenantBlobStorageConfiguration:AccountKeySecretName": "[parameters('tenancyStorageAccountKeyName')]",

        "TenantCosmosContainerFactoryOptions:RootTenantCosmosConfiguration:AccountUri": "[parameters('workflowCosmosAccountUri')]",
        "TenantCosmosContainerFactoryOptions:RootTenantCosmosConfiguration:KeyVaultName": "[parameters('workflowCosmosKeyVaultName')]",
        "TenantCosmosContainerFactoryOptions:RootTenantCosmosConfiguration:AccountKeySecretName": "[parameters('workflowCosmosAccountKeySecretName')]",
        "TenantCosmosContainerFactoryOptions:RootTenantCosmosConfiguration:DatabaseName": "[parameters('workflowCosmosDatabaseName')]"
      }
    }
  ],
  "outputs": {
  }
}
